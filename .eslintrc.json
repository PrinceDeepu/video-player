{
    "env": {
        "browser": true,
        "es2021": false
    },
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "script"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-console": "warn"
    }
}
